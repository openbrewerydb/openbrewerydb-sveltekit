{
  "name": "openbrewerydb-sveltekit",
  "version": "1.1.0",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "authors:build": "node scripts/build-authors.mjs",
    "changelogs:build": "node scripts/build-changelogs.mjs",
    "contributors:build": "node scripts/build-contributors.mjs",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint --ignore-pattern .gitignore .",
    "lint:fix": "prettier --check . && eslint --ignore-pattern .gitignore . --fix",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "test:report": "playwright show-report"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-cloudflare": "^7.2.8",
    "@sveltejs/kit": "^2.53.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.24",
    "brace-expansion": "^5.0.2",
    "devalue": "^5.6.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.15.0",
    "just-debounce": "^1.1.0",
    "lucide-svelte": "^0.575.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.5.0",
    "rehype-add-classes": "^1.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-urls": "^1.2.0",
    "remark-abbr": "^1.4.2",
    "svelte": "^5.53.0",
    "svelte-check": "^4.4.3",
    "svelte-eslint-parser": "^1.4.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^10.39.0",
    "@tailwindcss/vite": "^4.2.0",
    "d3-geo": "^3.1.1",
    "d3-scale-chromatic": "^3.1.0",
    "layerchart": "^1.0.13",
    "picomatch": "^4.0.3",
    "topojson-client": "^3.1.0",
    "us-atlas": "^3.0.1"
  }
}
